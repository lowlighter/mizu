{
  "name": "@mizu/code",
  "version": "0.2.0",
  "exports": {
    ".": "./mod.ts"
  },
  "tasks": {
    // Build dynamic imports and the list of mapped languages
    "build": "deno run --allow-read --allow-net=api.github.com --allow-write=mapping.json,import/highlight.js build.ts"
  },
  "imports": {
    "highlight.js": "npm:highlight.js@11"
  },
  "fmt": {
    "exclude": [
      "mapping.json"
    ]
  }
}
